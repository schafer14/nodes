<!DOCTYPE html>
<html>
<head>
	<title><%= settings.title %></title>
	<link rel="stylesheet" type="text/css" href="/stylesheets/style.css">
</head>
<body>
	<h1>Photos</h1>
	<p>Express stock photo application.</p>
	<div id="photos">
		<% photos.forEach(function(photo) { %>
			<div class="photo">
				<h2><%= photo.name %></h2>
				<img src="/photos/<%= photo.path%>"> <br />
				<a href="/photos/<%= photo.id %>/delete">Delete</a>
			</div>
		<% }) %>
	</div>
</body>
</html>